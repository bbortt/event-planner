{
  "name": "event-planner",
  "description": "Event Planning Tool.",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "build": "next build",
    "dev": "npm run next",
    "export": "next export -o build/static",
    "flow": "flow",
    "flow:check": "flow check",
    "fullbuild": "npm run build && npm run export",
    "gen": "graphql-codegen --config codegen.yml",
    "gen:watch": "graphql-codegen --config codegen.yml --watch",
    "lint": "next lint",
    "next": "next",
    "prepare": "owl install",
    "prettier": "prettier \"{,src/**/}*.{md,java,json,y(a)?ml,html,js,ts,tsx,css,scss}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "release": "standard-version",
    "start": "next start",
    "test": "jest --updateSnapshot --watch",
    "test:ci": "jest --ci --coverage --json --outputFile=build/jest-report.json --testLocationInResults --verbose",
    "pretest:e2e": "playwright install --with-deps",
    "test:e2e": "playwright test --config=playwright.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@auth0/auth0-react": "^1.9.0",
    "date-fns": "^2.28.0",
    "deepmerge": "^4.2.2",
    "foundation-sites": "^6.7.4",
    "graphql": "^16.3.0",
    "jquery": "^3.6.0",
    "lib": "file:src/main/webapp/lib",
    "next": "^12.1.0",
    "next-redux-wrapper": "^7.0.5",
    "pages": "file:src/pages",
    "react": "^18.0.0",
    "react-datepicker": "^4.7.0",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@bbortt/owl": "^1.0.4",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/flow": "^2.2.5",
    "@playwright/test": "^1.20.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "flow-bin": "^0.172.0",
    "flow-typed": "^3.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.6.1",
    "prettier-plugin-java": "^1.6.1",
    "pretty-quick": "^3.1.3",
    "sonar-web-frontend-reporters": "^3.4.0",
    "standard-version": "^9.3.2"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "jestSonar": {
    "reportPath": "build/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
