{
  "name": "event-planner",
  "description": "Event Planning Tool.",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "build": "next build",
    "dev": "npm run next",
    "export": "next export -o build/static",
    "flow": "flow",
    "flow:check": "flow check",
    "fullbuild": "npm run build && npm run export",
    "gen": "graphql-codegen --config codegen.yml",
    "gen:watch": "graphql-codegen --config codegen.yml --watch",
    "lint": "next lint",
    "next": "next",
    "prepare": "owl install",
    "prettier": "prettier \"{,src/**/}*.{md,java,json,y(a)?ml,html,js,ts,tsx,css,scss}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "start": "next start",
    "test": "jest --updateSnapshot --watch",
    "test:ci": "jest --ci --coverage --json --outputFile=build/jest-report.json --testLocationInResults --verbose",
    "pretest:e2e": "playwright install --with-deps",
    "test:e2e": "playwright test --config=playwright.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@auth0/auth0-react": "^1.11.0",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "foundation-sites": "^6.7.5",
    "graphql": "^16.6.0",
    "jquery": "^3.6.1",
    "lib": "file:src/main/webapp/lib",
    "next": "^12.3.0",
    "pages": "file:src/pages",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "sass": "^1.54.9"
  },
  "devDependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.19.0",
    "@bbortt/owl": "^1.0.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/flow": "^2.3.3",
    "@playwright/test": "^1.25.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.0.3",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-plugin-testing-library": "^5.6.4",
    "flow-bin": "^0.186.0",
    "flow-typed": "^3.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-junit": "^14.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-java": "^1.6.2",
    "pretty-quick": "^3.1.3",
    "sonar-web-frontend-reporters": "^3.4.0"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "jestSonar": {
    "reportPath": "build/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
