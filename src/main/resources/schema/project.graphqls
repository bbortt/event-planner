input ProjectCreateInput {
  name: String!
  description: String
  startDate: Date!
  endDate: Date!
}

input ProjectUpdateInput {
  id: ID!
  name: String
  description: String
  archived: Boolean
}

type Project {
  id: ID!
  token: String!
  name: String!
  description: String
  startDate: Date!
  endDate: Date!
  archived: Boolean!
  members: [Member!]!
  localities: [Locality!]!
}
