input CreateProjectInput {
  name: String!
  description: String
  startTime: DateTime!
  endTime: DateTime!
}

input ProjectUpdateInput {
  id: ID!
  name: String
  description: String
  archived: Boolean
}

type Project {
  id: ID!
  token: String!
  name: String!
  description: String
  startTime: DateTime!
  endTime: DateTime!
  archived: Boolean!
  members: [Member]
  localities: [Locality]
}
