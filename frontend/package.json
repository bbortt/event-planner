{
  "name": "event-planner",
  "version": "1.0.0-SNAPSHOT",
  "description": "Event Planning Tool.",
  "private": true,
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,ts,css,scss,yml}\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "ngc": "ngc -p tsconfig.app.json",
    "cleanup": "rimraf build/resources/main/static/",
    "clean-www": "rimraf build/resources/main/static/app/{src,build/}",
    "start": "npm run webpack:dev",
    "serve": "npm run start",
    "build": "npm run webpack:prod",
    "test": "npm run lint && jest --coverage --logHeapUsage -w=2 --config src/test/javascript/jest.conf.js",
    "test:watch": "npm run test -- --watch",
    "webpack:dev": "npm run webpack serve -- --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --env stats=minimal",
    "webpack:dev-verbose": "npm run webpack serve -- --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --progress --env stats=normal",
    "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --env stats=minimal",
    "webpack:build": "npm run cleanup && npm run webpack:build:main",
    "webpack:prod:main": "npm run webpack -- --config webpack/webpack.prod.js",
    "webpack:prod": "npm run cleanup && npm run webpack:prod:main && npm run clean-www",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack:test": "npm run test",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "@angular/common": "^11.2.6",
    "@angular/compiler": "^11.2.6",
    "@angular/core": "^11.2.6",
    "@angular/forms": "^11.2.6",
    "@angular/localize": "^11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/material-moment-adapter": "^11.2.5",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@angular/router": "^11.2.6",
    "@fortawesome/angular-fontawesome": "0.8.2",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "4.6.0",
    "devextreme": "^20.2.6",
    "devextreme-angular": "^20.2.6",
    "moment": "2.29.1",
    "ng-jhipster": "^0.16.0",
    "ngx-cookie-service": "11.0.2",
    "ngx-infinite-scroll": "10.0.1",
    "ngx-webstorage": "7.0.1",
    "popper.js": "1.16.1",
    "rxjs": "6.6.6",
    "sockjs-client": "^1.5.0",
    "swagger-ui-dist": "3.45.0",
    "tslib": "2.1.0",
    "webstomp-client": "^1.2.6",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "^11.2.5",
    "@angular/compiler-cli": "^11.2.6",
    "@ngtools/webpack": "^11.2.5",
    "@openapitools/openapi-generator-cli": "^2.2.2",
    "@types/jest": "26.0.21",
    "@types/node": "14.14.35",
    "@types/sockjs-client": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "autoprefixer": "10.2.5",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "2.3.0",
    "codelyzer": "^6.0.1",
    "copy-webpack-plugin": "8.0.0",
    "css-loader": "<4.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-loader": "4.0.2",
    "file-loader": "6.2.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.1",
    "husky": "^5.0.9",
    "jest": "26.6.3",
    "jest-date-mock": "1.0.8",
    "jest-junit": "12.0.0",
    "jest-preset-angular": "8.4.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "10.5.4",
    "merge-jsons-webpack-plugin": "1.0.21",
    "mini-css-extract-plugin": "1.3.9",
    "moment-locales-webpack-plugin": "1.2.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "postcss": "8.2.8",
    "postcss-loader": "5.2.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sass": "1.32.8",
    "sass-loader": "11.0.1",
    "simple-progress-webpack-plugin": "1.1.2",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.1.1",
    "thread-loader": "3.0.1",
    "to-string-loader": "1.1.6",
    "ts-loader": "8.0.18",
    "tslint": "6.1.3",
    "typescript": "4.2.3",
    "webpack": "5.26.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3",
    "webpack-notifier": "1.13.0",
    "workbox-webpack-plugin": "6.1.2",
    "write-file-webpack-plugin": "4.5.1"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "jestSonar": {
    "reportPath": "build/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
